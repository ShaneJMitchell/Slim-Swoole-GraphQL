{
    "name": "shmit/slim-swoole",
    "type": "project",
    "description": "Slim with Swoole boilerplate project.",
    "keywords": [
        "boilerplate",
        "slim",
        "swoole"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "monolog/monolog": "^1.24",
        "slim/slim": "^3.0",
        "vlucas/phpdotenv": "^3.3",
        "webonyx/graphql-php": "^0.13.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "types\\": "app\\types",
            "queries\\": "app\\queries",
            "models\\": "app\\models",
            "mutations\\": "app\\mutations",
            "repositories\\": "app\\repositories",
            "controllers\\": "app\\controllers",
            "resolvers\\": "app\\resolvers"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
